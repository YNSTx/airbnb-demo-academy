<% @reservations.each do |reservation| %>
  <div>
    <h2><%= reservation.lecture.title %></h2>
    <p><%= reservation.lecture.description %></p>
    <%= link_to 'Cancel Reservation', lecture_reservation_path(reservation.lecture, reservation), method: :delete %>
  </div>
<% end %>

<%= link_to 'Back to lectures', lectures_path %>
